#include <stdio.h>
#include <R.h>
#include <Rinternals.h>

void print_fibonacci(int n)
{
    if (n < 0) 
    {
        printf("invalid input\n");
        return;
    }
    int prev = 0;
    int curr = 1;
    for (int i = 0; i < n; ++i)
    {
        int tmp = curr + prev;
        prev = curr;
        curr = tmp;
    }
    printf("%d\n", curr);
}
